{
  "name": "zendesk-api-client",
  "version": "1.0.0",
  "description": "A Node.js client for interacting with Zendesk API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "export": "node export-tickets.js",
    "kpis": "node analyze-kpis.js",
    "transitions": "node analyze-transitions.js",
    "staff": "node analyze-staff.js",
    "voice": "node analyze-voice.js",
    "livefeed": "node livefeed.js",
    "livefeed:watch": "node livefeed.js --continuous",
    "web-sync": "node web/sync-data.js",
    "web-watch": "node web/sync-data.js watch",
    "web-serve": "cd web && python -m http.server 8080",
    "dashboard": "npm run web-sync && npm run web-serve",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "zendesk",
    "api",
    "client",
    "nodejs",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
